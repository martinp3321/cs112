////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
	//Compare inherit
////////////////////////////////////////////////////////////////////////////////////////////////////////////////////	
	
public class Song implements Comparable<Song>

{
	
	String songTitle;
	String genre;	
	
	String songPath;
	
	String artist;
	String albumName;

////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
	//Song object
////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
	
	Song(String title, String artist, String album, String genre, String path)
	
	{
		this.artist = artist;
		this.albumName = album;
		this.songTitle = title;
		this.genre = genre;
		
		
		
		this.songPath = path;
	}
////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
	//Search string lib
////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

	public int compareTo(Song songNext)
	
	{
		int comparable=0;
		
		if (this.songTitle.compareTo(songNext.songTitle)<0)
		{
			comparable = -1;
		}
		
		if (this.songTitle.compareTo(songNext.songTitle)>0)
		{
			comparable = 1;
		}
		
		if (this.songTitle.compareTo(songNext.songTitle)==0)
		{
			comparable = 0;
		}
		
		return comparable;
	}

}
