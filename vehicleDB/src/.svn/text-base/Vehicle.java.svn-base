
public abstract class Vehicle 
{
	
	
	protected static int numVehicles = 0;
	
	protected int numWheels;
	
	protected String manufacturer;
	
	
	public Vehicle(String manufacturer, int numWheels) 
	{
		this.manufacturer = manufacturer;
		this.numWheels = numWheels;
	}
	
	//get
	
	protected String getClassName() 
	{
		return "Vehicle";
	}
	
	protected String getManufacturer() 
	{
		return manufacturer;
	}



	protected int getNumWheels() 
	{
		return numWheels;
	}
	
	protected static int getNumVehicles() 
	{
		return numVehicles;
	}
	//set
	protected static void setNumVehicles(int numVehicles) 
	{
		Vehicle.numVehicles = numVehicles;
	}

	protected void setNumWheels(int numWheels) 
	{
		this.numWheels = numWheels;
	}
	
	protected void setManufacturer(String manufacturer) 
	{
		this.manufacturer = manufacturer;
	}



	

	
	//Override
	public String toString() 
	{
		return "Vehicle [manufacturer=" + manufacturer + ", numWheels=" + numWheels + "]";
	}
	
}
